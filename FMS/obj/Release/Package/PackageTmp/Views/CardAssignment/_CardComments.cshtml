@model FMS.Models.tbl_card_assignments
           
<div class="clsmodalheader">View Card Closed Comments</div>

<div style="overflow: auto; z-index: 9999; height: 350px;" id="divViewCardAssignment">
    @if (Model != null)
    {
    <table class="table table-striped table-condensed dTableR dataTable">
        <tr>
            <td style="width:20%">
               <b> Card# :</b>
            </td>
            <td style="width:80%">
              <span class="label-important"> @Html.Encode(Model.CardNo) </span> 
            </td>
        </tr>
        <tr>
            <td style="width:20%">
               <b> Comment :</b>
            </td>
            <td style="width:80%">
                @Html.Encode(Model.Comment)
            </td>
        </tr>
    </table>
    }
</div>
