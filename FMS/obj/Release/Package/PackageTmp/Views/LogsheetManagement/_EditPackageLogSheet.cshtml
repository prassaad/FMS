            $("#Package").focus();
            return false;
        }
        $("#SpanValidateMsg").hide();
        GetPackageRateByVehType(ClientID, $(this).val(), VehModelId, PackModel);
    });
    // Pack Model Change event
    $('#PackModel').change(function () {
