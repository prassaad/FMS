@model FMS.Models.tbl_vehicle_types

@{
    ViewBag.Title = "Create Vehicle Type";
}

<h2>Create Vehicle Type</h2>
<div class="bread_title">
                    <div class="bread_navigation">
                        <ul>
                           <li>@Html.ActionLink("Home","Index","Home")</li>
                            <li class="lastactive">@Html.ActionLink("List of Types", "Index")</li>
                        </ul>
                    </div>
            </div>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm("Create was unsuccessful. Please correct the errors and try again.")) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Vehicle Type</legend>

        <div class="editor-label">
            Vehicle Type : <span style="color:Red;">*</span>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.VehicleType)
            @Html.ValidationMessageFor(model => model.VehicleType)
        </div>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}
