@model IEnumerable<FMS.Models.tbl_penalties>
 
<table class="table table-striped table-condensed dTableR dataTable">
    <tr>
        <th>
            Date
        </th>
        <th>
            Log Sheet#
        </th>
        <th>
            Penality Amount
        </th>
        <th>
            Remark
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LogSheetNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PenalityAmt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remark)
            </td>
            
        </tr>
    }
</table>
