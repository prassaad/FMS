@model FMS.Models.CustomModel
           @using System.Data;
           @{
               System.Data.DataTable dt = (System.Data.DataTable)ViewBag.LogSheetFormatDt;
           }
<style>
    #tbl_list tr th
    {
    	text-align:left;
    }
</style>
@if (Model._headerList == null)
{
    <h2 style="text-align:center;">
        No Structure has Mapped
    </h2>
}
else
{
    <table class="table table-striped table-condensed dTableR dataTable" id="tbl_list">
        <tr>
            @foreach (var headerItem in Model._headerList)
            {
                <th>
                    @headerItem.Text.ToUpper()
                </th>
                }
        </tr>
        @foreach (DataRow dr in dt.Rows)
        {
            <tr>
                @foreach (DataColumn dc in dt.Columns)
                {
                    <td>
                        @dr[dc.ColumnName]
                    </td> 
                    }
            </tr>
            }
    </table>
}
