<div class="clsmodalheader"> Add Package Log Sheet</div>
@using (Html.BeginForm("AddPackageLogSheet", "LogsheetManagement", FormMethod.Post, new { id = "frmLogSheet", enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
