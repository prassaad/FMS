@model FMS.Models.tbl_vehicle_types

@{
    ViewBag.Title = "Edit Vehicle Type";
}

<h2>Edit Vehicle Type</h2>
<div class="bread_title">
                    <div class="bread_navigation">
                        <ul>
                            <li>@Html.ActionLink("Home","Index","Home")</li>
                            <li class="lastactive">@Html.ActionLink("List of Types", "Index")</li>
                        </ul>
                    </div>
            </div>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Edit Vehicle Type</legend>

        @Html.HiddenFor(model => model.ID)

        <div class="editor-label">
            Vehicle Type : <span style="color:Red;">*</span>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.VehicleType)
            @Html.ValidationMessageFor(model => model.VehicleType)
        </div>

        

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}
