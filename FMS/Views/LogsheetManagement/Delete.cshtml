@model FMS.Models.tbl_log_sheets

@{
    ViewBag.Title = "Delete Log Entry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete Duty Log Sheet</h2>
<div class="bread_title">
    <div class="bread_navigation">
        <ul>
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li class="active">@Html.ActionLink("List of Duty LogSheet", "Index")</li>
        </ul>
    </div>
</div>
<h3>Are you sure you want to delete this @Model.LogSheetNum Log Sheet?</h3>
@using (Html.BeginForm()) {
<p>
    <input type="submit" value="Delete" />
</p>
}
